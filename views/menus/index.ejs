<%- include('../partials/header') %>

<table id="menus">
    <thead>
        <tr>
            <th>Menu Name</th>
            <th>Menu Type</th>
        </tr>
    </thead>
    <tbody>
        <% menus.forEach(function(m) { %>
            <tr>
                <td><%= m.name %></td>
                <td><%= m.menuType %></td>
                <td></td>
            </tr>
            
            <% }); %>
    </tbody>
</table>


<%- include('../partials/footer') %>